<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raflisalam.magang">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Magang">
        <activity android:name=".ui.detail.DetailPelangganActivity"></activity>
        <activity android:name=".ui.fragment.pangkep.DetailPangkepActivity" />
        <activity android:name=".ui.fragment.maros.DetailMarosActivity" />
        <activity android:name=".ui.fragment.daya.DetailDayaActivity" />
        <activity android:name=".ui.fragment.karebosi.DetailKarebosiActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>